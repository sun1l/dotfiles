# set default ls
alias ls='ls -FGlAhp'

# get rid of command not found
alias cd..='cd ..'
alias ..='cd ..'

# set vim as default
alias vi=vim

# better safe then sorry
# confirmation
alias rm='rm -i'
alias mv='mv -iv'
alias cp='cp -iv'
alias ln='ln -i'

# common directories
alias cdsites='cd ~/Sites/'
